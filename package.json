{
  "name": "acFrontEnd",
  "version": "3.0.0",
  "description": "ac frontEnd solution",
  "main": "app.js",
  "scripts": {
    "mock": "json-server ./mock/db.js --port 4444",
    "dev": "webpack-dev-server --devtool eval --progress --colors",
    "start": "webpack-dev-server",
    "server2": "node server/index.js",
    "run2": "webpack-dev-server --progress --colors --config webpack.dev.config.js",
    "dev2": "webpack --progress --colors --config webpack.dev.config.js",
    "build2": "cross-env NODE_ENV='production' webpack -p --progress --profile --colors --config webpack.prod.config.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "jsdoc": "jsdoc ./scripts/*.js --readme ./README.md -t ./node_modules/ink-docstrap/template -c ./conf.json",
    "fixJsStandard": "standard --fix scripts/**/*.js"
  },
  "author": "anchao",
  "repository": {
    "type": "git",
    "url": "https://github.com/pvfhv/example-usegulp-cmd.git"
  },
  "keywords": [
    "gulp",
    "webpack",
    "sass",
    "compass",
    "react",
    "redux",
    "immutable",
    "babel",
    "es6",
    "bootstrap"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pvfhv/example-usegulp-cmd/issues"
  },
  "homepage": "https://github.com/pvfhv/example-usegulp-cmd",
