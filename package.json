  "main": "app.js",
  "scripts": {
    "precommit": "cp build/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit",
    "prepush": "npm run lintall",
    "server": "nodemon server/index.js",
    "predev": "npm run precommit",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --devtool eval --progress --config ./build/webpack.dev.config.js",
    "lint": "cross-env sh build/pre-commit || true",
    "lintall": "sh build/pre-commit lintall || true",
    "postlintall": "echo 'lintallall complete'",
    "prod": "echo 'production env'",
    "jsdoc": "jsdoc ./scripts/*.js --readme ./README.md -t ./node_modules/ink-docstrap/template -c ./conf.json",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
